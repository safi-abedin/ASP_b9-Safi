
<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - StackOverFlow</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/AdminLTE/plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/AdminLTE/css/adminlte.min.css">

    <link rel="stylesheet" src="~/stackoverflow/stacks-editor/dist/styles.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@@stackoverflow/stacks-editor/dist/styles.css" />
    <link rel="stylesheet" href="https://unpkg.com/@@stackoverflow/stacks/dist/css/stacks.min.css">

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/StackOverFlow.Web.styles.css" asp-append-version="true" />

    <style>
        /* Custom Styles */
        .navbar {
            border-top: 6px solid orange;
            margin-top: 0px;
        }

        .navbar-brand {
            text-decoration: none;
            color: black;
        }

        .navbar-toggler {
            border: none;
        }

            .navbar-toggler:focus {
                outline: none;
            }

        .menu-item {
            margin-right: 20px;
        }

        .logo-text {
            margin-left: 6px;
            text-decoration: none;
        }

            .logo-text:hover {
                text-decoration: none;
                color: black;
            }


        .border-after-navbar {
            border-top: 1px solid #dee2e6;
            margin-top: 0px;
        }

        .custom-search-input {
            border-radius: 12px;
            border: 1px solid #ced4da;
            padding: 10px;
            transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
        }

        .custom-search-input:focus {
            border-color: #007bff;
            outline: 0;
            box-shadow: 0 0 0 0.25rem rgba(0, 123, 255, 0.25);
        }


        .main-footer {
            margin-top: auto; /* Push the footer to the bottom */
            background-color: black;
            color: white;
            padding: 20px;
            width: 100%;
        }

        .footer-content {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .footer-logo img {
            width: 120px; /* Adjust size as needed */
            height: auto;
        }

        .footer-links {
            display: flex;
        }

        .footer-category {
            margin-right: 40px;
        }

        .category-header {
            font-weight: bold;
            margin-bottom: 10px;
            display: block;
        }

        .footer-category a {
            display: block;
            margin-bottom: 5px;
            color: #6a737c; /* Dark grey colour */
            text-decoration: none;
        }

            .footer-category a:hover {
                text-decoration: underline;
            }

        .copyright {
            margin-top: 20px;
            font-size: 12px;
            color: #6a737c;
        }

        .footer-logo a {
            display: flex;
            align-items: center; /* Align items vertically */
            color: inherit; /* Ensure the link colour inherits from its parent */
            text-decoration: none; /* Remove underline */
        }

        .footer-logo img {
            width: auto; /* Allow the width to adjust based on the content */
            height: 5em; /* Set the height to match the font size of the text */
            margin-right: 10px; /* Add spacing between the logo and text */
        }

        .logo-textFooter {
            font-size: 30px; /* Adjust font size as needed */
        }


    </style>
    @await RenderSectionAsync("Styles", required: false)
</head>
<body class="hold-transition sidebar-collapse layout-top-nav">
    <div class="wrapper">

        <nav class="main-header navbar navbar-expand-md layout-navbar-fixed navbar-light bg-white s-topbar">
            <div class="container s-topbar--container">
                <div class="row w-100">
                    <div class="col-1">
                        <!-- Optional: Add content if needed -->
                    </div>
                    <div class="col-4 d-flex justify-content-between align-items-center">
                        <button class="navbar-toggler order-1" type="button" data-toggle="open" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>

                        <div class="collapse navbar-collapse order-3" id="navbarCollapse">
                            <!-- Left navbar links -->
                            <ul class="navbar-nav">
                                <li class="nav-item">
                                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                                </li>
                            </ul>

                        </div>
                        <!-- Logo and Product/About Buttons -->
                        <a class="navbar-brand d-flex align-items-center" asp-area="" asp-controller="Home" asp-action="Index">
                            <img src="~/logo4.png" alt="Stack Overflow Logo" class="img-fluid align-self-center mt-1 s-topbar--item" style="max-width: 60px; height:50px;">
                            <span class="logo-text s-topbar--item" style="font-size: 2.0rem; margin-left: 8px;">Stack<span style="font-weight: bold;">Overflow</span></span>
                        </a>

                    </div>
                    <div class="col-6 d-flex justify-content-end align-items-center">

                        <a class="navbar-brand s-topbar--item" style="font-size:1rem" href="#">Product</a>
                        <a class="navbar-brand s-topbar--item" style="font-size:1rem" href="#">About</a>
                        <!-- Search Part -->
                        <form class="d-flex flex-grow-2 me-1 s-topbar--item">
                            <div class="input-group input-group-sm s-topbar--item">
                                <button class="btn btn-navbar " type="submit">
                                    <i class="fas fa-search"></i>
                                </button>
                                <input class="form-control me-2 flex-grow-1 custom-search-input " type="search" placeholder="Search" aria-label="Search">
                            </div>
                        </form>
                        <!-- LoginPartial -->
                        <div>
                            <partial name="_LoginPartial" />
                        </div>
                    </div>
                </div>
            </div>
        </nav>
        <!-- Add border after navbar -->
        <div class="border-after-navbar"></div>





        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-light-dark elevation-4">
    

            <!-- Sidebar -->
            <div class="sidebar">

                <!-- SidebarSearch Form -->
                <div class="form-inline">
                    <div class="input-group" data-widget="sidebar-search">
                        <input class="form-control form-control-sidebar" type="search" placeholder="Search" aria-label="Search">
                        <div class="input-group-append">
                            <button class="btn btn-sidebar">
                                <i class="fas fa-search fa-fw"></i>
                            </button>
                        </div>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-3">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                        <br></br>
                        <li class="nav-item">
                            <a href="../calendar.html" class="nav-link">
                                <i class="nav-icon far fa-calendar-alt"></i>
                                <p>
                                    Home
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="../gallery.html" class="nav-link">
                                <i class="nav-icon far fa-image"></i>
                                <p>
                                    Questions
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="../kanban.html" class="nav-link">
                                <i class="nav-icon fas fa-columns"></i>
                                <p>
                                    Tags
                                </p>
                            </a>
                        </li>
                    </ul>
                    <br></br>
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                        <li class="nav-header"></li>
                        <li class="nav-item">
                            <a href="../calendar.html" class="nav-link">
                                <i class="nav-icon far fa-calendar-alt"></i>
                                <p>
                                    Saves
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="../gallery.html" class="nav-link">
                                <i class="nav-icon far fa-image"></i>
                                <p>
                                    Users
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="../kanban.html" class="nav-link">
                                <i class="nav-icon fas fa-columns"></i>
                                <p>
                                    Companies
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">

            <!-- Main content -->
            <div class="content">
                <div class="container">
                    @RenderBody()
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->

    </div>

    <footer class="main-footer">
        <div class="footer-content">
            <div class="footer-logo">
                <a href="https://stackoverflow.com/">
                    <img src="~/logo4.png" alt="Stack Overflow Logo">
                    <span class="logo-textFooter">Stack Overflow</span>
                </a>

            </div>
            <div class="footer-links">
                <div class="footer-category">
                    <span class="category-header">STACK OVERFLOW</span>
                    <a href="https://stackoverflow.com/questions">Questions</a>
                    <a href="https://stackoverflow.com/help">Help</a>
                </div>
                <div class="footer-category">
                    <span class="category-header">PRODUCTS</span>
                    <a href="https://stackoverflow.com/teams">Teams</a>
                    <a href="https://stackoverflow.com/advertising">Advertising</a>
                    <a href="https://stackoverflow.com/collectives">Collectives</a>
                    <a href="https://stackoverflow.com/talent">Talent</a>
                </div>
                <div class="footer-category">
                    <span class="category-header">COMPANY</span>
                    <a href="https://stackoverflow.com/company/about">About</a>
                    <a href="https://stackoverflow.com/company/press">Press</a>
                    <a href="https://stackoverflow.com/company/work-here">Work Here</a>
                    <a href="https://stackoverflow.com/legal">Legal</a>
                </div>
                <div class="footer-category">
                    <span class="category-header">STACK EXCHANGE NETWORK</span>
                    <a href="https://stackoverflow.com/legal/privacy-policy">Privacy Policy</a>
                    <a href="https://stackoverflow.com/legal/terms-of-service/public">Terms of Service</a>
                    <a href="https://stackoverflow.com/company/contact">Contact Us</a>
                    <a href="https://stackoverflow.com/legal/privacy-policy#cookie-settings">Cookie Settings</a>
                    <a href="https://stackoverflow.com/legal/cookie-policy">Cookie Policy</a>
                </div>
            </div>
        </div>
        <div class="copyright">© 2024 Stack Exchange Inc; user contributions licensed under CC BY-SA. rev 2024.4.3.7257</div>
    </footer>


    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="~/AdminLTE/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/AdminLTE/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/AdminLTE/js/adminlte.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@@stackoverflow/stacks-editor/dist/app.bundle.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

