
<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - StackOverFlow</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/AdminLTE/plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/AdminLTE/css/adminlte.min.css">

    <link rel="stylesheet" src="~/stackoverflow/stacks-editor/dist/styles.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@@stackoverflow/stacks-editor/dist/styles.css" />
    <link rel="stylesheet" href="https://unpkg.com/@@stackoverflow/stacks/dist/css/stacks.min.css">

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/StackOverFlow.Web.styles.css" asp-append-version="true" />
    @await RenderSectionAsync("Styles", required: false)
</head>
<body class="hold-transition sidebar-collapse layout-top-nav">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand-md layout-navbar-fixed navbar-light bg-white ">
            <div class="container ">
                <div class="row w-100">
                    <div class="col-1">
                        <!-- Optional: Add content if needed -->
                    </div>
                    <div class="col-4 d-flex justify-content-between align-items-center">
                        <button class="navbar-toggler order-1" type="button" data-toggle="open" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>

                        <div class="collapse navbar-collapse order-3" id="navbarCollapse">
                            <!-- Left navbar links -->
                            <ul class="navbar-nav">
                                <li class="nav-item">
                                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                                </li>
                            </ul>

                        </div>
                        <!-- Logo and Product/About Buttons -->
                        <a class="navbar-brand d-flex align-items-center" asp-area="" asp-controller="Home" asp-action="Index">
                            <img src="~/logo4.png" alt="Stack Overflow Logo" class="img-fluid align-self-center mt-1 s-topbar--item" style="max-width: 60px; height:50px;">
                            <span class="logo-text s-topbar--item" style="font-size: 2.0rem; margin-left: 8px;">Stack<span style="font-weight: bold;">Overflow</span></span>
                        </a>

                    </div>
                    <div class="col-6 d-flex justify-content-end align-items-center">

                        <a class="navbar-brand s-topbar--item" style="font-size:1rem" href="#">Product</a>
                        <a class="navbar-brand s-topbar--item" style="font-size:1rem" href="#">About</a>
                        <!-- Search Part -->
                        <form class="d-flex flex-grow-2 me-1 s-topbar--item">
                            <div class="input-group input-group-sm s-topbar--item">
                                <button class="btn btn-navbar " type="submit">
                                    <i class="fas fa-search"></i>
                                </button>
                                <input class="form-control me-2 flex-grow-1 custom-search-input " type="search" placeholder="Search" aria-label="Search">
                            </div>
                        </form>
                        <!-- LoginPartial -->
                        <div>
                            <partial name="_LoginPartial" />
                        </div>
                    </div>
                </div>
            </div>
        </nav>
        <!-- Add border after navbar -->
        <div class="border-after-navbar"></div>



        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-light-dark elevation-4">
  
            <!-- Sidebar -->
            <div class="sidebar">

                <!-- Sidebar Menu -->
                <nav class="mt-3">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                        <br />
                        <li class="nav-item">
                            <a  asp-action="Index" asp-controller="Home" class="nav-link">
                                <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path fill-rule="evenodd" clip-rule="evenodd" d="M15 10V15.0004C15 16.1047 14.098 17 12.9915 17H5.00853C3.89925 17 3 16.1126 3 15.0004V10H0L9 1L18 10H15ZM7 11V17H11V11H7Z" fill="black" />
                                </svg>
                                <p>
                                    Home
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-area="User" asp-controller="Questions" asp-action="index" class="nav-link">
                                <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M4 15L1 18V4C1 2.9 1.9 2 3 2H15C16.0893 2 17 2.91067 17 4V13C17 14.0893 16.0893 15 15 15H4ZM11.75 11.03C12.4681 10.1997 12.73 9.1746 12.73 8.09C12.73 6.43874 12.0227 4.87481 10.4238 4.25938C9.49912 3.90349 8.33186 3.90525 7.40679 4.25876C5.86053 4.84965 4.98984 6.46093 5.09 8.09C5.09 9.38 5.44 10.38 6.12 11.09C6.8684 11.8244 7.92321 12.2204 8.97 12.16C9.59 12.16 10.17 12.05 10.68 11.82C11.33 12.26 11.68 12.5 11.74 12.52C11.97 12.65 12.2 12.75 12.44 12.82L13.03 11.69C12.5748 11.5304 12.144 11.3083 11.75 11.03ZM10.48 10.13C10.0254 9.78581 9.5191 9.5158 8.98 9.33L8.53 10.23C8.86 10.35 9.19 10.52 9.51 10.73C9.31 10.8 9.09 10.84 8.86 10.84C8.25 10.84 7.74 10.61 7.34 10.16C6.47643 9.1669 6.47638 7.04317 7.34 6.05C8.14101 5.1487 9.6891 5.14836 10.49 6.05C11.3922 7.06498 11.3547 9.08037 10.48 10.13Z" fill="black" />
                                </svg>

                                <p>
                                    Questions
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="../kanban.html" class="nav-link">
                                <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path opacity="0.4" d="M9.24265 1C8.447 1 7.68394 1.31604 7.12133 1.87866L1.41422 7.58582C0.789633 8.21033 0.664511 9.14526 1.03885 9.89417C1.18523 9.52441 1.408 9.17786 1.70711 8.87866L7.70711 2.87866C8.26972 2.31604 9.03278 2 9.82844 2H14C14.2731 2 14.5376 2.0365 14.789 2.10486C14.4605 1.44971 13.7828 1 13 1H9.24265Z" fill="black" />
                                    <path fill-rule="evenodd" clip-rule="evenodd" d="M9.82843 3C9.298 3 8.78929 3.21071 8.41421 3.58579L2.41422 9.5858C1.63317 10.3668 1.63317 11.6332 2.41422 12.4142L6.58579 16.5858C7.36684 17.3668 8.63317 17.3668 9.41422 16.5858L15.4142 10.5858C15.7893 10.2107 16 9.70202 16 9.17159V5C16 3.89543 15.1046 3 14 3L9.82843 3ZM12 9C10.8954 9 10 8.10457 10 7C10 5.89543 10.8954 5 12 5C13.1046 5 14 5.89543 14 7C14 8.10457 13.1046 9 12 9Z" fill="black" />
                                </svg>
                                <p>
                                    Tags
                                </p>
                            </a>
                        </li>
                    </ul>
                    <br></br>
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                        <li class="nav-header"></li>
                        <li class="nav-item">
                            <a href="../calendar.html" class="nav-link">
                                <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path fill-rule="evenodd" clip-rule="evenodd" d="M3 1C1.9 1 1 1.9 1 3V15C1 16.1016 1.89844 17 3 17H15C16.1 17 17 16.1 17 15V7C17 6.73478 16.8946 6.48043 16.7071 6.29289L11.7071 1.29289C11.5196 1.10536 11.2652 1 11 1H3ZM3 3H11V6H3V3ZM9 14C10.1046 14 11 13.1046 11 12C11 10.8954 10.1046 10 9 10C7.89543 10 7 10.8954 7 12C7 13.1046 7.89543 14 9 14Z" fill="black" />
                                </svg>
                                <p>
                                    Saves
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="../gallery.html" class="nav-link">
                                <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M17 13.999C17 14.438 16.55 14.999 16 14.999H8.995C8.456 14.999 8.001 14.552 8 14H2C1.46 14 1 13.439 1 13C1 10.366 4 9 4 9C4 9 4.229 8.591 4 8C3.159 7.379 2.942 7.41 3 5C3.058 2.581 4.367 2 5.5 2C6.633 2 7.942 2.58 8 5C8.058 7.41 7.841 7.379 7 8C6.771 8.59 7 9 7 9C7 9 8.549 9.711 9.42 11.088C10.196 10.369 11 9.999 11 9.999C11 9.999 11.229 9.59 11 8.999C10.159 8.379 9.942 8.409 10 5.999C10.058 3.58 11.367 2.999 12.5 2.999C13.633 2.999 14.937 3.58 14.995 5.999C15.054 8.409 14.837 8.379 13.995 8.999C13.766 9.589 13.995 9.999 13.995 9.999C13.995 9.999 17 11.365 17 13.999Z" fill="black" />
                                </svg>
                                <p>
                                    Users
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="../kanban.html" class="nav-link">
                                <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M10 16V12H8V16H2V4C2 2.9 2.9 2 4 2H10C11.0893 2 12 2.91067 12 4V6H14C15.0893 6 16 6.91067 16 8V16H10ZM4 4V6H6V4H4ZM4 8V10H6V8H4ZM8 4V6H10V4H8ZM8 8V10H10V8H8ZM4 12V14H6V12H4ZM12 12V14H14V12H12ZM12 8V10H14V8H12Z" fill="black" />
                                </svg>
                                <p>
                                    Companies
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <div class="content-wrapper">
            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>

    </div>

    <footer class="main-footer">
        <div class="footer-content">
            <div class="footer-logo">
                <a href="https://stackoverflow.com/">
                    <img src="~/logo4.png" alt="Stack Overflow Logo">
                    <span class="logo-textFooter">Stack Overflow</span>
                </a>

            </div>
            <div class="footer-links">
                <div class="footer-category">
                    <span class="category-header">STACK OVERFLOW</span>
                    <a href="https://stackoverflow.com/questions">Questions</a>
                    <a href="https://stackoverflow.com/help">Help</a>
                </div>
                <div class="footer-category">
                    <span class="category-header">PRODUCTS</span>
                    <a href="https://stackoverflow.com/teams">Teams</a>
                    <a href="https://stackoverflow.com/advertising">Advertising</a>
                    <a href="https://stackoverflow.com/collectives">Collectives</a>
                    <a href="https://stackoverflow.com/talent">Talent</a>
                </div>
                <div class="footer-category">
                    <span class="category-header">COMPANY</span>
                    <a href="https://stackoverflow.com/company/about">About</a>
                    <a href="https://stackoverflow.com/company/press">Press</a>
                    <a href="https://stackoverflow.com/company/work-here">Work Here</a>
                    <a href="https://stackoverflow.com/legal">Legal</a>
                </div>
                <div class="footer-category">
                    <span class="category-header">STACK EXCHANGE NETWORK</span>
                    <a href="https://stackoverflow.com/legal/privacy-policy">Privacy Policy</a>
                    <a href="https://stackoverflow.com/legal/terms-of-service/public">Terms of Service</a>
                    <a href="https://stackoverflow.com/company/contact">Contact Us</a>
                    <a href="https://stackoverflow.com/legal/privacy-policy#cookie-settings">Cookie Settings</a>
                    <a href="https://stackoverflow.com/legal/cookie-policy">Cookie Policy</a>
                </div>
            </div>
        </div>
        <div class="copyright">© 2024 Stack Exchange Inc; user contributions licensed under CC BY-SA. rev 2024.4.3.7257</div>
    </footer>


    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="~/AdminLTE/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/AdminLTE/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/AdminLTE/js/adminlte.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@@stackoverflow/stacks-editor/dist/app.bundle.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

